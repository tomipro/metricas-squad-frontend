{
  "name": "flight-booking-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@types/node": "^16.18.38",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^27.5.2",
    "identity-obj-proxy": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false --coverageReporters=lcov --coverageReporters=text",
    "test:executive": "react-scripts test --testPathPattern=ExecutiveSummary --coverage",
    "test:operations": "react-scripts test --testPathPattern=Operations --coverage",
    "test:both": "react-scripts test --testPathPattern='(ExecutiveSummary|Operations)' --coverage",
    "test:all": "react-scripts test --coverage",
    "test:watch": "react-scripts test --watch",
    "test:watch:executive": "react-scripts test --testPathPattern=ExecutiveSummary --watch",
    "test:watch:operations": "react-scripts test --testPathPattern=Operations --watch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
